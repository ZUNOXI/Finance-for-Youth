<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.ssafy.javer.DTO.Board_dto" alias="Board_dto"></typeAlias>
		<typeAlias type="com.ssafy.javer.DTO.Comment_dto" alias="Comment_dto"></typeAlias>
		<typeAlias type="com.ssafy.javer.DTO.ReplyBoard_dto" alias="ReplyBoard_dto"></typeAlias>
		<typeAlias type="com.ssafy.javer.DTO.ReplyComment_dto" alias="ReplyComment_dto"></typeAlias>
		<typeAlias type="com.ssafy.javer.DTO.Member" alias="member" />
		<typeAlias type="com.ssafy.javer.DTO.News_dto" alias="News_dto" />
		<typeAlias type="com.ssafy.javer.DTO.Stock_dto" alias="Stock_dto" />
	</typeAliases>
  <mappers>
    <mapper resource="mybatis/BoardSQLMapper.xml"/>
    <mapper resource="mybatis/CommentSQLMapper.xml"/>
    <mapper resource="mybatis/ReplyBoardSQLMapper.xml"/>
    <mapper resource="mybatis/ReplyCommentSQLMapper.xml"/>
    <mapper resource="mybatis/Member.xml" />	
    <mapper resource="mybatis/NewsSQLMapper.xml" />	
    <mapper resource="mybatis/StockSQLMapper.xml" />	
  </mappers>
</configuration>